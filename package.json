{
  "name": "react-luna",
  "description": "React UI Kit",
  "version": "0.0.1-alpha.6",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "react-ui",
    "luna"
  ],
  "author": "Andrea Montorio <andrea.montorio@gmail.com>",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "babel": "babel src/lib -d lib",
    "sass": "sass src/lib/index.scss:lib/index.css src/lib/theme.css:lib/theme.css --no-source-map",
    "prepublishOnly": "npm run sass && npm run babel"
  },
  "peerDependencies": {
    "react": "^16 || ^17"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "gh-pages": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loskware/react-luna.git"
  },
  "homepage": "https://loskware.github.io/react-luna/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
